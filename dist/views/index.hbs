    <title>{{texts.gpNameDefault}}</title>

    <link rel="stylesheet" type="text/css"
        href="https://cdn.rawgit.com/noppa/text-security/master/dist/text-security.css">
    {{!-- <script src="https://cdn.tailwindcss.com"></script> --}}

